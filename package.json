{
  "name": "imtool",
  "version": "1.1.2",
  "description": "Client-side canvas-based image manipulation library.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "thumbnail",
    "react",
    "front-end",
    "image",
    "typescript",
    "canvas",
    "image manipulation",
    "screenshot"
  ],
  "scripts": {
    "prepare": "npm run build",
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mat-sz/imtool.git"
  },
  "bugs": {
    "url": "https://github.com/mat-sz/imtool/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "__tests__/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "homepage": "https://github.com/mat-sz/imtool",
  "author": "mat-sz <contact@matsz.dev>",
  "license": "BSD-3-Clause-Clear",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "canvas": "^2.6.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  }
}
